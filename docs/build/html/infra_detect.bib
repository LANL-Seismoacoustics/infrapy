	Journal = {Journal of Geophysical Research: Atmospheres},
	Keywords = {infrasound detection; infrasound propagation},
	Number = {13},
	Pages = {7773-7798},
	Title = {Automatic infrasound detection and location of sources in the western United States},
	Url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1002/2013JD021084},
