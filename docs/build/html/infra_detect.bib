@article{Brown:2002,
	Abstract = { --- This paper describes an automatic and interactive data processing system designed to locate impulsive atmospheric sources with a yield of at least one kiloton by detecting and characterizing the airborne infrasound radiated by the source. The infrasonic processing subsystem forms part of a larger system currently under development at the Prototype International Data Center (PIDC) in Arlington, Virginia where seismic, hydroacoustic, radionuclide and infrasonic methods are used to detect and locate impulsive sources in any terrestrial environment. Infrasonic signal detection is achieved via a coincidence detector which requires both the normalized cross correlation and the short-term-average/long-term-average ratio of a beam in the direction of maximum correlation to exceed predetermined threshold values simultaneously before a detection is declared. The infrasound propagation model currently used to infer travel-time information assumes the horizontal sound speed across the ground to be 320.0{\thinspace}m/s. This crude model is currently being replaced by a model which predicts travel-time information through a ray-tracing algorithm for acoustic waves in an atmosphere with seasonal representations for temperature and wind. A novel feature of the source location process is the fusion of all available arrival information, whether it be seismic, hydroacoustic or infrasonic to locate a single source where it is reasonable to hypothesize a common source. In its final configuration the infrasonic subsystem will routinely process data from the global 60-station International Monitoring System (IMS) infrasonic network currently under development.},
	Annote = {IDC Goal is to locate 1-kT sources at distances of 2500 km.  The IDC location algorithm uses arrival-dependent weights which are the inverses of the a priori measurements and modeling errors of each error.  The infrasonic travel-time model does not take into account wind along propagation paths, therefore, a large a priori error is assigned to the travel time, and the travel-time component of infrasonic arrivals is given a lower weight than the travel time component of seismic arrivals.  The azimuth of infrasonic arrivals is assigned a low a priori error, therefore it is given more weight.
Signal detection is with a coincidence detector which relies on the normalized cross correlation and the STA/LTA ratio of a beam directed toward the region of the slowness plane with high correlation.  },
	Author = {Brown, D. J. and Katz, C. N. and Le Bras, R. and Flanagan, M. P. and Wang, J. and Gault, A. K.},
	Date-Added = {2018-03-19 22:32:49 +0000},
	Date-Modified = {2018-03-20 21:56:24 +0000},
	Day = {01},
	Doi = {10.1007/s00024-002-8674-2},
	Issn = {1420-9136},
	Journal = {pure and applied geophysics},
	Keywords = {infrasound detection; infrasound association; infrasound location},
	Month = {Mar},
	Number = {5},
	Pages = {1081--1125},
	Title = {Infrasonic Signal Detection and Source Location at the Prototype International Data Centre},
	Url = {https://doi.org/10.1007/s00024-002-8674-2},
	Volume = {159},
	Year = {2002},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s00024-002-8674-2}}

