	Issn = {1944-8007},
	Journal = {Geophysical Research Letters},
	Keywords = {spectral analysis, covariance matrix, ambient seismic noise, array processing},
	Note = {2016GL070320},
	Number = {18},
	Pages = {9644--9652},
	Title = {Spatial coherence of the seismic wavefield continuously recorded by the USArray},
	Url = {http://dx.doi.org/10.1002/2016GL070320},
	Volume = {43},
	Year = {2016},
